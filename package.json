{
  "name": "meteo",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "nodemon --require dotenv/config --watch server --exec tsx server/index.ts",
    "build": "npm run build:client && npm run build:server && npm run build:entry-server",
    "build:client": "vite build --outDir dist/client --minify false",
    "build:entry-server": "vite build --ssr src/entry-server.tsx --outDir dist/entry-server --minify false",
    "build:server": "tsup",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "test": "vitest run",
    "prepare": "husky",
    "redis:start": "docker-compose -f ./docker/redis-docker-compose.yml --env-file .env up"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.5",
    "clsx": "^2.1.1",
    "compression": "^1.8.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "maplibre-gl": "^5.13.0",
    "maplibre-react-components": "^0.2.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "redis": "^5.10.0",
    "sha1": "^1.1.1",
    "sirv": "^3.0.2",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/react": "^16.1.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.6",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/sha1": "^1.1.5",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^5.1.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "nodemon": "^3.1.11",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  }
}
